{
  "ci": {
    "collect": {
      "url": ["http://localhost/", "http://localhost/reading/", "http://localhost/photos/"],
      "staticDistDir": "./public"
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "tap-targets": "off",
        "max-potential-fid": "off",
        "render-blocking-resources": "off",
        "uses-http2": "off"
      }
    }
  }
}
