{
  "ci": {
    "collect": {
      "url": ["http://localhost/", "http://localhost/reading/", "http://localhost/photos/", "http://localhost/talks/"],
      "staticDistDir": "./public"
    },
    "assert": {
      "preset": "lighthouse:no-pwa",
      "assertions": {
        "tap-targets": "off",
        "max-potential-fid": "off",
        "render-blocking-resources": "off",
        "uses-http2": "off",
        "uses-rel-preload": "off",
        "uses-rel-preconnect": "off",
        "csp-xss": "off",
        "lcp-lazy-loaded": "off",
        "bf-cache": "off"
      }
    }
  }
}
